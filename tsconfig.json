{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "classic",
    "forceConsistentCasingInFileNames": true, 
    "noUnusedLocals": true,
    "noUnusedParameters": true 
    },
  "include": ["./src/**/*"]
}
